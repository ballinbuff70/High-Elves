<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <FactionDef>
	<defName>HE_ElvenNPFaction</defName>
	<label>High Elves</label>
	<fixedName>Kingdom of the Northstar</fixedName>
    <description>The Empire of Man. The men and women of the Empire represent human civilization. They stand firm against the greenskins, skaven, chaos, and all other threats to order. Builders, magic users, warriors, researchers, all manner of civilians, and more make them up.</description>
    <colorSpectrum>
      <li>(0.35, 0.35, 0.35)</li>
      <li>(0.5, 0.50, 0.50)</li>
    </colorSpectrum>
    <pawnsPlural>humans</pawnsPlural>
    <settlementGenerationWeight>0.5</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>High Loremaster</leaderTitle>
    <factionIconPath>Things/World/HighElvesIconPlayer</factionIconPath>
	<maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <startingGoodwill>
      <min>30</min>
      <max>70</max>
    </startingGoodwill>
	<settlementNameMaker>NamerHighElvesCities</settlementNameMaker>
	<techLevel>Medieval</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>HE_Backstories_Standard</li>
        </categories>
      </li>
    </backstoryFilters>
	<geneticVariance>0.6</geneticVariance>
	<hairTags>
		<li>Urban</li>
		<li>Tribal</li>
	</hairTags>
    <naturalColonyGoodwill>
      <min>0</min>
      <max>0</max>
    </naturalColonyGoodwill>
	<!-- TODO
    <caravanTraderKinds>
      <li>RH_TET_Dwarf_Caravan_BulkGoods</li>
      <li>RH_TET_Dwarf_Caravan_CombatSupplier</li>
      <li>RH_TET_Dwarf_Caravan_Mercenaries</li>
    </caravanTraderKinds>
	-->
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-40</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <pawnGroupMakers>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
			<RH_TET_Empire_Civilian>100</RH_TET_Empire_Civilian>
		</options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <disallowedStrategies>
          <li>Siege</li>
        </disallowedStrategies>
        <options>
			<HighElf_PWNKND_Citizen>100</HighElf_PWNKND_Citizen>
		</options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <HighElf_PWNKND_Citizen>1</HighElf_PWNKND_Citizen>
        </traders>
        <carriers>
          <RH_TET_Horse>6</RH_TET_Horse>
        </carriers>
        <guards>
			<HighElf_PWNKND_Citizen>100</HighElf_PWNKND_Citizen>
		</guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
			<HighElf_PWNKND_Citizen>100</HighElf_PWNKND_Citizen>
		</options>
      </li>
    </pawnGroupMakers>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
  </FactionDef>

</Defs>