        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <startingGoodwill>
      <min>30</min>
      <max>70</max>
    </startingGoodwill>
	<settlementNameMaker>NamerHighElvesCities</settlementNameMaker>
	<techLevel>Medieval</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>HE_Backstories_Standard</li>
        </categories>
      </li>
    </backstoryFilters>
	<geneticVariance>0.6</geneticVariance>
