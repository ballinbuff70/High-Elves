<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- HE_Archer -->
  
  <ThingDef ParentName="ArmorSmithableBase">
    <defName>Apparel_HE_Archer</defName>
    <label>High Elf Archer Armor</label>
    <description>PLACEHOLDER</description>
    <techLevel>Medieval</techLevel>
    <graphicData>
      <texPath>Things/Apparel/Middle/HE_Archer/HE_Archer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <color>(255, 255, 255)</color>
    </graphicData>
    <costList>
		<Steel>20</Steel>
		<Resource_IthilmarIngot>1</Resource_IthilmarIngot>
    </costList>
	<thingCategories>
      <li>Apparel</li>
    </thingCategories>
		<statBases>
				<WorkToMake>1200</WorkToMake>
				<MaxHitPoints>500</MaxHitPoints>
				<Mass>1</Mass>
				<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
				<ArmorRating_Sharp>0.25</ArmorRating_Sharp>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
	 <equippedStatOffsets>
      <WorkSpeedGlobal>0.1</WorkSpeedGlobal>
	  <MoveSpeed>0.1</MoveSpeed>
    </equippedStatOffsets>	
    <recipeMaker>
      <recipeUsers>
        <li>HighElves_Smithery</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
		<apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Middle/HE_Archer/HE_Archer</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
		
		<li>HE_Clothing_Archer_High</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

