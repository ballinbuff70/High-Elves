  <ThingDef ParentName="ArmorSmithableBase">
    <defName>Apparel_HE_Mage</defName>
    <label>High Elf Mage Armor</label>
    <description>High Elf Mages are aspirants to the Magical arts, but due to their natural affinity to the Winds of Magic, they are far more powerful than even the most experienced Imperial Battle Wizards.</description>
    <techLevel>Medieval</techLevel>
    <graphicData>
      <texPath>Things/Apparel/Shell/HE_Mage/HE_Mage</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <color>(255, 255, 255)</color>
    </graphicData>
    <costList>
		<Steel>120</Steel>
		<Resource_IthilmarIngot>15</Resource_IthilmarIngot>
    </costList>
	<thingCategories>
      <li>Apparel</li>
    </thingCategories>
	<statBases>
			<WorkToMake>2400</WorkToMake>
			<MaxHitPoints>800</MaxHitPoints>
			<Mass>1.25</Mass>
			<ArmorRating_Blunt>0.35</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.65</ArmorRating_Sharp>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
	 <equippedStatOffsets>
      <WorkSpeedGlobal>0.1</WorkSpeedGlobal>
	  <MoveSpeed>0.1</MoveSpeed>
    </equippedStatOffsets>	
    <recipeMaker>
      <recipeUsers>
        <li>HighElves_MagicStation</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>10</Crafting>
      </skillRequirements>
    </recipeMaker>
		<apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Shell/HE_Mage/HE_Mage</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
		<li>HE_Clothing_Magic_Mid</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

