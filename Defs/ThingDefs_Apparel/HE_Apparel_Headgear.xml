  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>Apparel_HE_MageHeadpiece</defName>
    <label>High Elf Mage Headpiece</label>
    <description>PLACEHOLDER</description>
    <techLevel>Medieval</techLevel>
    <graphicData>
      <texPath>Things/Apparel/Headgear/HE_MageMinor/HE_MageMinor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <color>(255, 255, 255)</color>
    </graphicData>
    <costList>
		<Steel>120</Steel>
		<Gold>10</Gold>
		<Resource_IthilmarIngot>15</Resource_IthilmarIngot>
    </costList>
    <thingCategories>
      <li>Headgear</li>
    </thingCategories>
	<statBases>
				<WorkToMake>1700</WorkToMake>
				<MaxHitPoints>350</MaxHitPoints>
				<Mass>1.5</Mass>
				<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
				<ArmorRating_Sharp>0.65</ArmorRating_Sharp>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <recipeMaker>
      <recipeUsers>
        <li>HighElves_MagicStation</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
		<apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Headgear/HE_MageMinor/HE_MageMinor</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
		<li>MedievalMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
	<comps>
		<li Class="TorannMagic.Enchantment.CompProperties_EnchantedItem">
			<hasEnchantment>true</hasEnchantment>
			<arcaneSpectre>true</arcaneSpectre>
			<maxMP>0.07</maxMP>
			<maxMPTier>Standard</maxMPTier>
			<mpRegenRate>0.05</mpRegenRate>
			<mpRegenRateTier>Standard</mpRegenRateTier>
			<mpCost>0.05</mpCost>
			<mpCostTier>Negative</mpCostTier>
			<arcaneRes>0.1</arcaneRes>
			<arcaneResTier>Standard</arcaneResTier>
		</li>
	</comps>	
  </ThingDef>

