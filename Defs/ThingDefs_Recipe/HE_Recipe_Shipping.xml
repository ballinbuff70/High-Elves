<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Selling Crates -->

  <RecipeDef>
    <defName>Ship_CornCrates</defName>
    <label>Ship Corn Crate</label>
    <description>Ship Corn Crate, a higher return than caravaning the shipment.</description>
    <jobString>Ship Corn Crate</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	<recipeUsers>
		<li>HighElves_DockStation</li>
	</recipeUsers>
    <ingredients>
	  <li>
        <filter>
          <thingDefs>
            <li>CornCrates</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CornCrates</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Gold>135</Gold>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

