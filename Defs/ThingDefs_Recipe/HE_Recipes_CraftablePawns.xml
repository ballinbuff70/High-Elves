<Defs>

	<RecipeDef>
		<defName>CPF_Make_DRSKT_RacePawn</defName>
		<label>Contract a High Elf recruit</label>
		<description>Contract a High Elf recruit.</description>
		<jobString>Contracting a High Elf recruit</jobString>
		<recipeUsers>
			<li>HighElves_RequisitionsTent</li>
		</recipeUsers>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Smith</soundWorking>
		<workAmount>2000</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<ingredients>	
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>150</count>
			</li>
		</ingredients>
		<products>
			<CPF_DRSKT_RacePawnAssembler>1</CPF_DRSKT_RacePawnAssembler>
			<!--Should be equal to the defName of your assembly kit item-->
		</products>
	</RecipeDef>

