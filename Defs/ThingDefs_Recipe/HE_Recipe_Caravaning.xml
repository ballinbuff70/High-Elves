<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Selling Crates -->

  <RecipeDef>
    <defName>Caravan_CornCrates</defName>
    <label>Caravan Corn Crate</label>
    <description>Caravan Corn Crate</description>
    <jobString>Caravan Corn Crate</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	<recipeUsers>
		<li>HighElves_RequisitionsTent</li>
	</recipeUsers>
    <ingredients>
	  <li>
        <filter>
          <thingDefs>
            <li>CornCrates</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CornCrates</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Gold>100</Gold>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

