<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Crate Packing and Unpacking -->

  <RecipeDef>
    <defName>Create_CornCrates</defName>
    <label>Create Corn Crate</label>
    <description>Create Corn Crate</description>
    <jobString>Create Corn Crate</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	<recipeUsers>
		<li>HighElves_DockStation</li>
	</recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawCorn</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawCorn</li>
		<li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <CornCrates>1</CornCrates>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

