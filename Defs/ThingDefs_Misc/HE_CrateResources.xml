<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>CornCrates</defName>
    <label>Corn Crates</label>
    <description>These are crated Corn boxes to be sold for a golden profit or stored for use at a later point in time. Be warned, half of what was packed will be lost if unpacked...</description>
    <graphicData>
      <texPath>Things/Items/Resources/CornCrates</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>200</MarketValue>
      <Mass>0.1</Mass>
      <Flammability>0.1</Flammability>
      <DeteriorationRate>0.1</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>

