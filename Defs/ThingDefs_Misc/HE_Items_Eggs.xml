<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="OrganicProductBase">
		<defName>CPF_DRSKT_RacePawnAssembler</defName>
		<label>High Elf Contract</label>
		<description>A contract for the hiring of a new High Elf member of the colony.</description>
		<!--All info related to the texture.-->
		<graphicData>
			<texPath>Things/Items/CraftablePawnEggs/Craftable_Egg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(245,245,245)</color> <!--Also known as white-->
			<drawSize>0.8</drawSize>
		</graphicData>
		<!--All info about if the item can be eaten, and what it should 'taste' like. -->
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<foodType>AnimalProduct</foodType>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<tasteThought>AteRawFood</tasteThought>
		</ingestible>
		<!--To what category does the item belong?-->
		<thingCategories>
			<li>EggsFertilized</li>
		</thingCategories>
		<socialPropernessMatters>true</socialPropernessMatters>
		<tickerType>Normal</tickerType>
		<!-- All the stats about the item itself. -->
		<statBases>
			<Mass>0.15</Mass>
			<MaxHitPoints>20</MaxHitPoints>
			<DeteriorationRate>2</DeteriorationRate>
			<Flammability>0.7</Flammability>
			<Nutrition>0.25</Nutrition>
			<MarketValue>23.0</MarketValue>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
		</statBases>
		<!--Comps declare what different C# code should be used. -->
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>15</daysToRotStart>
				<rotDestroys>true</rotDestroys>
				<disableIfHatcher>true</disableIfHatcher>
			</li>
			<li Class="CompProperties_TemperatureRuinable">
				<minSafeTemperature>0</minSafeTemperature>
				<maxSafeTemperature>50</maxSafeTemperature>
				<progressPerDegreePerTick>0.00003</progressPerDegreePerTick>
			</li>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>0.2</hatcherDaystoHatch> <!--0.0001 is instant-->
				<hatcherPawn>HighElf_PWNKND_Citizen</hatcherPawn> <!--Your PawnKind defName goes in here! -->
			</li>
		</comps>
	</ThingDef>

